then
    pp5=$(realpath  ${pp4})
elif [ -f /bin/readlink -o /usr/bin/readlink ]
then
    pp5=$(readlink -m  ${pp4})
fi

if [ ! -f ${pp5} ]
then
    echo
    echo
    echo ' no <noW_bin_exe> exist. exit. '
    echo
    exit
