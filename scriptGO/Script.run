then
    xx4=$(realpath  ${xx1}/../ )
elif [ -f /bin/readlink -o /usr/bin/readlink ]
then
    xx4=$(readlink -m  ${xx1}/../ )
fi

[ 1 = 1 ] || echo "export GOROOT=${xx4}"
                   export GOROOT=${xx4}

[ 1 = 1 ] || echo "${xx1}/${pp9}   $*"
                   ${xx1}/${pp9}   $*
